<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
    "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
  <!-- PRIVATE ZONE -->
  <rule>
    <from>/common/signmodule/requestPlugin/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)$</from>
    <set type="session" name="restOfTheUrlVar">P7P $3/$4/$5 P7P</set>
    <to last="true">/common/signmodule/requestPlugin/$1/$2?restOfTheUrl=$3/$4/$5/$6/$7</to>
  </rule>
  <rule>
    <from>/common/signmodule/requestPlugin/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)$</from>
    <set type="session" name="restOfTheUrlVar">P6P $3/$4/$5 P6P</set>
    <to last="true">/common/signmodule/requestPlugin/$1/$2?restOfTheUrl=$3/$4/$5/$6</to>
  </rule>
  <rule>
    <from>/common/signmodule/requestPlugin/(.*)/(.*)/(.*)/(.*)/(.*)$</from>
    <set type="session" name="restOfTheUrlVar">P5P $3/$4/$5 P5P</set>
    <to last="true">/common/signmodule/requestPlugin/$1/$2?restOfTheUrl=$3/$4/$5</to>
  </rule>
  <rule>
    <from>/common/signmodule/requestPlugin/(.*)/(.*)/(.*)/(.*)$</from>
    <set type="session" name="restOfTheUrlVar">P4P $3/$4 P4P</set>
    <to last="true">/common/signmodule/requestPlugin/$1/$2?restOfTheUrl=$3/$4</to>
  </rule>
  <rule>
    <from>/common/signmodule/requestPlugin/(.*)/(.*)/(.*)$</from>
    <set type="session" name="restOfTheUrlVar">P3P $3 P3P</set>
    <to last="true">/common/signmodule/requestPlugin/$1/$2?restOfTheUrl=$3</to>
  </rule>
  <!-- PUBLIC ZONE -->
  <rule>
    <from>/public/signmodule/requestPlugin/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)$</from>
    <set type="session" name="restOfTheUrlVar">P7P $3/$4/$5 P7P</set>
    <to last="true">/public/signmodule/requestPlugin/$1/$2?restOfTheUrl=$3/$4/$5/$6/$7</to>
  </rule>
  <rule>
    <from>/public/signmodule/requestPlugin/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)$</from>
    <set type="session" name="restOfTheUrlVar">P6P $3/$4/$5 P6P</set>
    <to last="true">/public/signmodule/requestPlugin/$1/$2?restOfTheUrl=$3/$4/$5/$6</to>
  </rule>
  <rule>
    <from>/public/signmodule/requestPlugin/(.*)/(.*)/(.*)/(.*)/(.*)$</from>
    <set type="session" name="restOfTheUrlVar">P5P $3/$4/$5 P5P</set>
    <to last="true">/public/signmodule/requestPlugin/$1/$2?restOfTheUrl=$3/$4/$5</to>
  </rule>
  <rule>
    <from>/public/signmodule/requestPlugin/(.*)/(.*)/(.*)/(.*)$</from>
    <set type="session" name="restOfTheUrlVar">P4P $3/$4 P4P</set>
    <to last="true">/public/signmodule/requestPlugin/$1/$2?restOfTheUrl=$3/$4</to>
  </rule>
  <rule>
    <from>/public/signmodule/requestPlugin/(.*)/(.*)/(.*)$</from>
    <set type="session" name="restOfTheUrlVar">P3P $3 P3P</set>
    <to last="true">/public/signmodule/requestPlugin/$1/$2?restOfTheUrl=$3</to>
  </rule>

</urlrewrite>
