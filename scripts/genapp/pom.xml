<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.fundaciobit.genapp</groupId>
    <version>2.0.0</version>
    <artifactId>genapp-run</artifactId>
    <packaging>pom</packaging>
    <name>FundacioBit GenApp - Run</name>
    <repositories>
        <repository>
            <id>genapp-maven-repos</id>
            <name>GovernIB Maven Repository</name>
            <url>https://governib.github.io/maven/maven/</url>
        </repository>
    </repositories>
    <dependencies>
        <dependency>
            <groupId>org.fundaciobit.genapp</groupId>
            <artifactId>genapp-generator</artifactId>
            <version>2.0.0-SNAPSHOT</version>
        </dependency>
    </dependencies>
    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>exec-maven-plugin</artifactId>
                <version>1.6.0</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>java</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <mainClass>org.fundaciobit.genapp.generator.gui.RebApp</mainClass>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>

