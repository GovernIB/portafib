<?xml version="1.0" encoding="UTF-8"?>
<datasources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns="http://www.ironjacamar.org/doc/schema"
             xsi:schemaLocation="http://www.ironjacamar.org/doc/schema
             http://www.ironjacamar.org/doc/schema/datasources_1_2.xsd">

	<datasource jndi-name="java:jboss/datasources/seyconDS" pool-name="seyconDS" enabled="true" use-java-context="true">
		<connection-url>jdbc:postgresql://localhost:5432/seycon</connection-url>
		<driver>postgresql</driver>
		<security>
			<user-name>seycon</user-name>
			<password>seycon</password>
		</security>
	</datasource>

</datasources>